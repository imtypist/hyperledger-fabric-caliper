{
    "name": "@hyperledger/caliper-gui-dashboard",
    "description": "Hyperledger Caliper GUI dashboard module, for easily assembling configuration files and monitoring benchmark runs",
    "version": "0.5.0",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/hyperledger/caliper",
        "directory": "packages/caliper-gui-dashboard"
    },
    "scripts": {
        "pretest": "npm run licchk",
        "licchk": "license-check-and-add",
        "test": "npm run lint",
        "lint": "npx eslint .",
        "nyc": "nyc mocha --recursive -t 10000",
        "start": "SKIP_PREFLIGHT_CHECK=true react-scripts start",
        "build": "react-scripts build",
        "eject": "react-scripts eject",
        "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
        "compile-sass": "node-sass src/assets/scss/paper-dashboard.scss src/assets/css/paper-dashboard.css",
        "minify-sass": "node-sass src/assets/scss/paper-dashboard.scss src/assets/css/paper-dashboard.min.css --output-style compressed",
        "map-sass": "node-sass src/assets/scss/paper-dashboard.scss src/assets/css/paper-dashboard.css --source-map true"
    },
    "engines": {
        "node": ">=14.19.0",
        "npm": ">=6.14.16"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.27",
        "@fortawesome/free-brands-svg-icons": "^5.12.1",
        "@fortawesome/free-solid-svg-icons": "^5.12.1",
        "@fortawesome/react-fontawesome": "^0.1.8",
        "axios": "0.21.2",
        "bootstrap": "4.4.1",
        "chart.js": "^2.9.3",
        "d3": "^5.15.0",
        "history": "4.10.1",
        "js-yaml": "^3.13.1",
        "mime-types": "^2.1.24",
        "node-sass": "7.0.1",
        "patternomaly": "^1.3.2",
        "perfect-scrollbar": "1.5.0",
        "react": "^16.12.0",
        "react-chartjs-2": "^2.9.0",
        "react-copy-to-clipboard": "^5.0.2",
        "react-d3-graph": "^2.3.0",
        "react-dom": "16.12.0",
        "react-google-maps": "9.4.5",
        "react-notification-alert": "0.0.12",
        "react-router": "5.1.2",
        "react-router-dom": "5.1.2",
        "react-scripts": "3.4.0",
        "react-textarea-autosize": "^7.1.2",
        "reactstrap": "8.4.1"
    },
    "devDependencies": {
        "babel-eslint": "10.0.1",
        "eslint": "^5.16.0",
        "eslint-plugin-react": "^7.18.3",
        "license-check-and-add": "2.3.6",
        "mocha": "3.4.2",
        "nyc": "11.1.0"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "optionalDependencies": {
        "@types/googlemaps": "3.39.3",
        "@types/markerclustererplus": "2.1.33",
        "@types/react": "16.9.23",
        "ajv": "6.12.0",
        "jquery": "3.4.1",
        "typescript": "3.8.2"
    },
    "license-check-and-add-config": {
        "folder": ".",
        "license": "../../LICENSE.txt",
        "exact_paths_method": "EXCLUDE",
        "exact_paths": [
            "node_modules",
            ".nyc_output",
            "coverage",
            ".gitignore",
            "log",
            ".jshintrc",
            "src/assets/.DS_Store"
        ],
        "file_type_method": "EXCLUDE",
        "file_types": [
            ".json",
            ".yml",
            ".md",
            ".html",
            ".php",
            ".log",
            ".css",
            ".scss",
            ".map",
            ".eot",
            ".ttf",
            ".woff",
            ".woff2"
        ],
        "insert_license": false,
        "license_formats": {
            "js|jsx": {
                "prepend": "/*",
                "append": "*/",
                "eachLine": {
                    "prepend": "* "
                }
            },
            "editorconfig|yaml": {
                "prepend": "#",
                "append": "#",
                "eachLine": {
                    "prepend": "# "
                }
            }
        }
    },
    "nyc": {
        "exclude": [
            "test/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": false,
        "statements": 1,
        "branches": 1,
        "functions": 1,
        "lines": 1
    },
    "license": "Apache-2.0"
}
